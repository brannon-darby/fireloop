{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins",
      "../node_modules/@mean-expert/loopback-stats-mixin/dist",
      "../node_modules/@mean-expert/loopback-component-realtime/dist/mixins",
      "../node_modules/loopback-ds-timestamp-mixin"
    ]
  },
  "User": {
    "dataSource": "db",
    "public": false
  },
  "AccessToken": {
    "dataSource": "db",
    "public": true,
    "relations": {
      "user": {
        "type": "belongsTo",
        "model": "Account",
        "foreignKey": "userId"
      }
    }
  },
  "ACL": {
    "dataSource": "db",
    "public": true
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": true
  },
  "Role": {
    "dataSource": "db",
    "public": true
  },
  "Todo": {
    "dataSource": "db",
    "public": true
  },
  "Note": {
    "dataSource": "db",
    "public": true
  },
  "Container": {
    "dataSource": "storage",
    "public": true
  },
  "Account": {
    "dataSource": "db",
    "public": true
  }
}
